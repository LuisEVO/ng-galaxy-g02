{
  "hosting": {
    "predeploy": "npm install && npm run build:shopping-card",
    "public": "dist/app-project-shopping-card",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
